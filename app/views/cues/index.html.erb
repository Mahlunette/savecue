<div class="container">
  <%= link_to @back, class: "bt btn-back" do %>
  <% end %>
  <% @msg.each_line do |s| %>
    <h1><%= s %></h1>
  <% end %>
  <div class="row">
  <% @cues.each do |cue| %>
    <%= link_to new_cue_user_cue_path(cue), class: "col-6 p-2" do %>
      <%= render partial: "shared/cue_card", locals: {cue: cue } %>
    <% end %>
  <% end %>
  </div>
</div>
